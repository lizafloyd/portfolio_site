{
  "name": "liza-floyd-portfolio",
  "description": "Version 2 of Lizas Portfolio site.",
  "author": "Liza Floyd",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "react-slick": "^0.23.2",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "*",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.3",
    "gh-pages": "^1.0.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "react-test-renderer": "^15.6.2",
    "sass-lint": "^1.12.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "yarn build && gh-pages -d build --repo git@github.com:LizaFloyd/portfolio_site.git",
    "format": "prettier --write './src/**/*.js'",
    "lint": "eslint ./src --cache && sass-lint './src/css/*.scss' --no-exit -v && prettier -l './src/**/*.js' './tests/**/*.js' || (echo need formatting, use yarn format ; exit 1)"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
